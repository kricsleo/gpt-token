{
  "name": "gpt-token",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@8.6.5",
  "description": "Calculate the number of text tokens in GPT.",
  "author": "kricsleo <kricsleo@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/kricsleo/gpt-token#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kricsleo/gpt-token.git"
  },
  "bugs": "https://github.com/kricsleo/gpt-token/issues",
  "keywords": [
    "chatgpt",
    "gpt",
    "tokens",
    "calculate"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@types/node": "^18.16.18",
    "bumpp": "^9.1.1",
    "pnpm": "^8.6.5",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "js-tiktoken": "^1.0.7"
  }
}
